<%= form_for(@ticket) do |f| %>
  <% if @ticket.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@ticket.errors.count, "error") %> prohibited this ticket from being saved:</h2>

      <ul>
      <% @ticket.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :start_railway_station, "Начальная" %><br>    
    <%= f.collection_select :start_railway_station_id, RailwayStation.all, :id, :title %>
  </div>
  
  <div class="field">
    <%= f.label :finish_railway_station, "Конечная" %><br>    
    <%= f.collection_select :finish_railway_station_id, RailwayStation.all, :id, :title %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>  
<% end %>
